{
  "name": "workout-tracker-api",
  "version": "0.0.1",
  "description": "TypeScript server that exposes workout data from Google Sheets",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development tsx watch --clear-screen=false src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.3.0",
    "googleapis": "^126.0.1",
    "mongodb": "^6.17.0",
    "node-cron": "^4.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.4.5",
    "@types/object-hash": "^3.0.6",
    "csv-parse": "^5.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
